<%- include('partials/header'); %> 
<div id="main" class="error404">
  <div class="container-fluid">
    <div class="row">
      <main role="main" class="col">
        <h1><%= error.status %></h1>
        <h3><%= message %></h3>
      </main>
    </div> <!-- end .row --> 
  </div><!-- end .container-fluid -->
</div><!-- end #main -->
<%- include('partials/footer'); %>


<% if (locals.errors && locals.errors.xyz) { %>
	<% locals.errors.xyz.forEach(function(error) { %>
		<p class="alert alert-primary" role="alert"><%= error %></p>
	<% }) %>
<% } %>
